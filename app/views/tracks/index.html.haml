%h1.page-header
  Tracks Matching #{params[:tracks_name]}

%ul
  - @tracks.each do |track|
    %li
      %iframe{ :src => "https://embed.spotify.com/?uri=spotify:track:#{track.id}",
                :frameborder => 0,
                :allowtransparency => true}
      |
      = form_tag "tracks/?track_id=#{track.id}", method: "post", html: { class: "form"} do
        =submit_tag "Add Track", class: "btn btn-primary"

